  <snippet name="actual_x_alpha">
   <global>prevX = 0
prevY = 0
is_first = true</global>
   <function>if (is_first) then
  is_first = false
  prevX = time
  prevY = value
end

dx = time - prevX
dy = value - prevY
prevX = time
prevY = value

return dy/dx</function>
   <linked_source>/my_robot/gen_vel/data.3</linked_source>
  </snippet>
