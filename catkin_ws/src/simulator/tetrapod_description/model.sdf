                <geometry>
                    <mesh>
                        <uri>model://tetrapod_description/Meshes/upper_leg.dae</uri>
                    </mesh>                    
                </geometry>
            </visual>
            <inertial>
                <!-- Position of upper_leg CoM relative to hip pitch joint position. Obtained from Solidworks-->
