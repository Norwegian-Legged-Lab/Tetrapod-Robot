                <mass>1.0</mass>
                <inertia>
                <ixx>0.1</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.1</iyy>
                <iyz>0.0</iyz>
                <izz>0.1</izz>
                </inertia>
            </inertial>
        </link>

        <joint name="front_left_hip_pitch" type="revolute">
