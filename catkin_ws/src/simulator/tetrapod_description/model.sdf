<?xml version="1.0"?>

<!--
Base to shoulder yaw:
dX = 201
dY = 205

Shoulder yaw to shoulder pitch:
dX = 129mm

Shoulder pitch to knee pitch:
dX = 329mm

Knee pitch to foot:
dX = 482mm (joint to foot center)
dX = 512mm (joint to tip)

-->

<!--Parameters
x_0 = 0.201
y_0 = 0.205
z_0 = 0.0


-->

<sdf version='1.7'>
    <model name="Tetrapod">
        <pose>0 0 1 0 0 0</pose>
        <static>false</static>

        <link name="body">
            <pose>0 0 0 0 0 0</pose>

            <inertial>
                <pose>0 0 0 0 0 0</pose>
                <mass>10.0</mass>
                <inertia>
                <ixx>1.0</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>1.0</iyy>
                <iyz>0.0</iyz>
                <izz>1.0</izz>
                </inertia>
            </inertial>

            <visual name="body_visual">
                <geometry>
                    <mesh>
                        <uri>model://tetrapod_description/Meshes/base.dae</uri>
                    </mesh>  
                </geometry>
            </visual>
            <collision name="body_collision">
                <geometry>
                    <box>
                        <size>0.5 0.5 0.15</size>
                    </box>
                </geometry>
            </collision>
        </link>


<!-- FRONT LEFT LEG -->

        <link name="front_left_hip">
            <!-- Let the pose of the link coincide with the joint position -->
            <!--
                x = x_0
                y = y_0
                z = z_0
                r = 0
                p = 0
                y = 0
            -->
            <pose>0.201 0.205 0 0 0 0</pose>

            <inertial>
                <!-- Position of front_left_hip CoM relative to joint position. Obtained from Solidworks-->
                <pose>0 0.0 0 0 0 0</pose>

                <mass>1.0</mass>
                <inertia>
                <ixx>0.1</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.1</iyy>
                <iyz>0.0</iyz>
                <izz>0.1</izz>
                </inertia>
            </inertial>

            <visual name="front_left_hip_visual">
                <geometry>
                    <mesh>
                        <uri>model://tetrapod_description/Meshes/left_shoulder.dae</uri>
                    </mesh>  
                </geometry>
            </visual>

        </link>

        <joint name="front_left_hip_yaw" type="revolute">
            <!-- Use zero as joint and link position coincide-->
            <pose>0 0 0 0 0 0</pose> 

            <child>front_left_hip</child>
            <parent>body</parent>
            <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <lower>0</lower>
                    <upper>3.14</upper>
                    <effort>30</effort>
                    <velocity>1000</velocity>
                </limit>
            </axis>
        </joint>

        <link name="front_left_upper_leg">
            <!-- Let the pose of the link coincide with the hip pitch joint position -->
            <!--
                x = x_0 + r1
                y = y_0
                z = z_0 + d1
                r = -1.5708 (rotate the leg)
                p = 0
                y = 0
            -->
            <pose>0.330 0.201 0 -1.5708 0 0</pose>

            <collision name="front_left_upper_leg_collision">
                <!-- Add pose for collision-->
                <pose>0.1645 0 0 0 1.5708 0</pose>
                <geometry>
                    <cylinder>
                        <radius>0.01</radius>
                        <length>0.329</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name="front_left_upper_leg">
