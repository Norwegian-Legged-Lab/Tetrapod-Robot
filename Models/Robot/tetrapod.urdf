        </inertial>
        <visual>
            <origin xyz="0.25 0.0 0.0" rpy="0.0 1.5708 0.0"/>
            <geometry>
                <cylinder length="0.5" radius="0.025"/>
            </geometry>
            <material name="red"/>
        </visual>
        
